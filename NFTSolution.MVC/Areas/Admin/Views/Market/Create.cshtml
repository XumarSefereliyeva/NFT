@using NFTSolution.DAL.Models
@model Market


            <!-- Form Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">Basic Form</h6>
                            <form asp-area="Admin" asp-controller="Market" asp-action="Create" method="post" enctype="multipart/form-data">
                                <div class="mb-3">
                                    <label for="exampleInputEmail1" class="form-label">Name</label>
                                    <input asp-for="Name"  class="form-control" id="exampleInputEmail1"
                                        aria-describedby="emailHelp">
                                    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.
                                    </div>
                                </div>
                                <div class="mb-3">
                                    <label for="exampleInputPassword1" class="form-label">Count</label>
                        <input asp-for="Count" class="form-control" id="exampleInputPassword1">
                                </div>
                                <div class="mb-3">
                                    <label for="exampleInputPassword1" class="form-label">Category</label>
                        <input asp-for="Category" class="form-control" id="exampleInputPassword1">
                                </div>
                                <div>
                         <label for="exampleInputPassword1" class="form-label">ImgUrl</label>
                    <input asp-for="ImgUrl" class="form-control" id="exampleInputPassword1">
            </div>
                               
                                <button type="submit" class="btn btn-primary">Create</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Form End -->

